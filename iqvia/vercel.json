{
    "builds": [{
        "src": "myapp/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.9" }
    }],
    "routes": [
        {
            "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
            "dest": "myapp/wsgi.py"
        }
    ]
}
